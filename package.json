{
  "name": "ors-ext-client",
  "sencha": {
    "namespace": "Ors",
    "type": "code",
    "toolkit": "",
    "creator": "anonymous",
    "summary": "Short summary",
    "detailedDescription": "Long description of package",
    "version": "1.0.0",
    "compatVersion": "1.0.0",
    "format": "1",
    "slicer": {
      "js": [
        {
          "path": "${package.dir}/sass/example/custom.js",
          "isWidgetManifest": true
        }
      ]
    },
    "output": "${package.dir}/build",
    "local": true,
    "sass": {
      "namespace": "Ors",
      "etc": [
        "${package.dir}/sass/etc/all.scss"
      ],
      "var": [
        "${package.dir}/sass/var"
      ],
      "src": [
        "${package.dir}/sass/src"
      ]
    },
    "classpath": [
      "${package.dir}/../GeoExt/src",
      "${package.dir}/../BasiGX/src",
      "${package.dir}/${toolkit.name}"
    ],
    "overrides": [
      "${package.dir}/overrides"
    ],
    "example": {
      "path": [
        "${package.dir}/examples"
      ]
    },
    "framework": "ext",
    "requires": [
      "ux"
    ],
    "builds": {
      "classic": {
        "toolkit": "classic",
        "theme": "theme-triton",
        "sass": {
          "generated": {
            "var": "classic/sass/save.scss",
            "src": "classic/sass/save"
          }
        }
      },
      "modern": {
        "toolkit": "modern",
        "theme": "theme-material",
        "sass": {
          "generated": {
            "var": "modern/sass/save.scss",
            "src": "modern/sass/save"
          }
        }
      }
    }
  },
  "scripts": {
    "test": "npm run lint && npm run test:spec",
    "test:spec": "karma start --single-run --reporters=mocha",
    "lint": "eslint -c .eslintrc src/ && eslint -c test/.eslintrc test/"
  },
  "dependencies": {},
  "peerDependencies": {
    "@geoext/geoext": "^4.0.0",
    "@geoext/openlayers-legacy": "^6.5.4",
    "@terrestris/basigx": "^3.0.0",
    "@terrestris/d3-util": "^0.9.3",
    "@turf/turf": "^5.1.6",
    "moment": "^2.17.1",
    "openrouteservice-js": "^0.1.11"
  },
  "devDependencies": {
    "@geoext/geoext": "^4.0.0",
    "@geoext/openlayers-legacy": "^6.5.4",
    "@terrestris/basigx": "^3.0.0",
    "@terrestris/d3-util": "^0.9.3",
    "@turf/turf": "^5.1.6",
    "eslint": "^7.30.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-expect": "^1.1.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "mocha": "^9.0.2",
    "moment": "^2.17.1",
    "openrouteservice-js": "^0.1.11",
    "puppeteer": "^5.5.0",
    "sinon": "^11.1.1"
  }
}
